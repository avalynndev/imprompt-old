{
  "name": "imprompt",
  "displayName": "Imprompt",
  "version": "0.0.1",
  "description": "A Chrome extension that enhances prompts on AI model websites.",
  "author": "avalynndev <avalynndev@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@google/generative-ai": "^0.24.1",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/redux-persist": "^6.1.0",
    "@plasmohq/storage": "^1.10.0",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^2.2.3",
    "@supabase/supabase-js": "^2.43.1",
    "ai": "^4.3.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jotai": "^2.8.3",
    "lucide-react": "^0.525.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-intersection-observer": "^9.10.1",
    "react-markdown": "^9.0.1",
    "react-textarea-autosize": "^8.5.3",
    "redux-persist-webextension-storage": "^1.0.2",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "path-browserify": "^1.0.0",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "process": "^0.11.10",
    "typescript": "5.3.3",
    "url": "^0.11.0"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "host_permissions": [
      "https://chatgpt.com/*",
      "https://claude.ai/*",
      "https://gemini.google.com/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "https://chatgpt.com/*",
          "https://claude.ai/*",
          "https://gemini.google.com/*"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}